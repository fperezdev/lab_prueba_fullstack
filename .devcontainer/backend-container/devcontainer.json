{
  "name": "Lab Prueba Backend",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "lab-prueba-backend",
  "shutdownAction": "none",
  "workspaceFolder": "/workspaces/backend",
  "forwardPorts": [3000, 5432],
  "portsAttributes": {
    "3000": {
      "label": "Lab Prueba Backend"
    },
    "5432": {
      "label": "Postgres"
    }
  },
  "customizations": {
    "vscode": {
      "settings": {
        "editor.formatOnSave": true
      },
      "extensions": [
        "usernamehw.errorlens",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "humao.rest-client"
      ]
    }
  }
}
